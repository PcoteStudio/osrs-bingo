{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Sqlite": {
    "ConnectionString": "Data Source={pathToData}\\localdb.db;",
    "CachePath": "{pathToData}\\cachedb.db"
  },
  "Session": {
    "CookieName": "Session",
    "ExpiryInMinutes": 120
  }
}
